(* Integers implemented using natural numbers *)
module A
  use HighOrd
  type nat = Z | S nat
  type z = P nat | N nat
  function neg (ds : z) : z =
    match ds with
    | P ds2 ->
        match ds2 with
        | Z -> P Z
        | S n -> N n
        end
    | N n2 -> P (S n2)
    end
  goal prop_neg_involutive : forall x : z . x = neg (neg x)
end

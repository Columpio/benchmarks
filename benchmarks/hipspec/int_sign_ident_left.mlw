(* Integers implemented using natural numbers *)
module A
  use HighOrd
  type sign = Pos | Neg
  function opposite (ds : sign) : sign =
    match ds with
    | Pos -> Neg
    | Neg -> Pos
    end
  function timesSigns (ds2 : sign) (x : sign) : sign =
    match ds2 with
    | Pos -> x
    | Neg -> opposite x
    end
  goal prop_sign_ident_left : forall s : sign . timesSigns s Pos = s
end

module A
  use HighOrd
  type nat = Z | S nat
  function plus (ds : nat) (y : nat) : nat =
    match ds with
    | Z -> y
    | S x -> S (plus x y)
    end
  function even (ds2 : nat) : bool =
    match ds2 with
    | Z -> True
    | S ds1 ->
        match ds1 with
        | Z -> False
        | S x2 -> even x2
        end
    end
  goal prop_T16 : forall x3 : nat . even (plus x3 x3) = True
end

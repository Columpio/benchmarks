module A
  use HighOrd
  type nat = Z | S nat
  type list 'a = Nil | Cons 'a (list 'a)
  function drop (ds : nat) (xs : list 'a2) : list 'a2 =
    match ds with
    | Z -> xs
    | S ipv ->
        match xs with
        | Nil -> Nil
        | Cons ipv1 ipv2 -> drop ipv ipv2
        end
    end
  goal prop_11 : forall xs2 : list 'a4 . drop Z xs2 = xs2
end

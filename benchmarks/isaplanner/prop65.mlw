module A
  use HighOrd
  type nat = Z | S nat
  function plus (ds : nat) (y : nat) : nat =
    match ds with
    | Z -> y
    | S x -> S (plus x y)
    end
  function lt (ds2 : nat) (ds1 : nat) : bool =
    match ds1 with
    | Z -> False
    | S ipv ->
        match ds2 with
        | Z -> True
        | S x2 -> lt x2 ipv
        end
    end
  goal prop_65 : forall i : nat, m : nat . lt i (S (plus m i)) = True
end

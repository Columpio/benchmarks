(* Source: IsaPlanner test suite *)
module A
  use HighOrd
  type nat = Z | S nat
  function minus (ds : nat) (ds1 : nat) : nat =
    match ds with
    | Z -> Z
    | S ipv ->
        match ds1 with
        | Z -> ds
        | S ipv1 -> minus ipv ipv1
        end
    end
  goal prop_79 :
    forall m : nat, n : nat, k : nat .
      minus (minus (S m) n) (S k) = minus (minus m n) k
end

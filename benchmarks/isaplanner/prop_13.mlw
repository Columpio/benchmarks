(* Source: IsaPlanner test suite *)
module A
  use HighOrd
  type nat = Z | S nat
  type list 'a = Nil | Cons 'a (list 'a)
  function drop (ds : nat) (xs : list 'a2) : list 'a2 =
    match ds with
    | Z -> xs
    | S ipv ->
        match xs with
        | Nil -> (Nil : list 'a2)
        | Cons ipv1 ipv2 -> drop ipv ipv2
        end
    end
  goal prop_13 :
    forall n : nat, x : 'a4, xs2 : list 'a4 .
      drop (S n) (Cons x xs2) = drop n xs2
end

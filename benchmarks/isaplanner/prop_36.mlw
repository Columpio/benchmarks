(* Source: IsaPlanner test suite *)
module A
  use HighOrd
  type list 'a = Nil | Cons 'a (list 'a)
  function constTrue (ds : 't) : bool = True
  function takeWhile
    (ds2 : ('a2 -> bool)) (ds1 : list 'a2) : list 'a2 =
    match ds1 with
    | Nil -> (Nil : list 'a2)
    | Cons x xs ->
        match ds2 x with
        | False -> (Nil : list 'a2)
        | True -> Cons x (takeWhile ds2 xs)
        end
    end
  goal prop_36 :
    forall xs2 : list 'a4 . takeWhile constTrue xs2 = xs2
end

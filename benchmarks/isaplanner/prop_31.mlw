(* Source: IsaPlanner test suite *)
module A
  use HighOrd
  type nat = Z | S nat
  function min (ds : nat) (y : nat) : nat =
    match ds with
    | Z -> Z
    | S x ->
        match y with
        | Z -> Z
        | S y1 -> S (min x y1)
        end
    end
  goal prop_31 :
    forall a : nat, b : nat, c : nat .
      min (min a b) c = min a (min b c)
end

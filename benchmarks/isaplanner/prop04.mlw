module A
  use HighOrd
  type nat = Z | S nat
  type list 'a = Nil | Cons 'a (list 'a)
  function equal2 (ds : nat) (ds1 : nat) : bool =
    match ds with
    | Z ->
        match ds1 with
        | Z -> True
        | S ipv -> False
        end
    | S ds2 ->
        match ds1 with
        | Z -> False
        | S y -> equal2 ds2 y
        end
    end
  function count (x : nat) (ds3 : list nat) : nat =
    match ds3 with
    | Nil -> Z
    | Cons y2 ys ->
        match equal2 x y2 with
        | False -> count x ys
        | True -> S (count x ys)
        end
    end
  goal prop_04 :
    forall n : nat, xs : list nat .
      S (count n xs) = count n (Cons n xs)
end

module A
  use HighOrd
  type list 'a = Nil | Cons 'a (list 'a)
  function constFalse (ds : 't) : bool = False
  function dropWhile
    (ds2 : ('a2 -> bool)) (ds1 : list 'a2) : list 'a2 =
    match ds1 with
    | Nil -> Nil
    | Cons x xs ->
        match ds2 x with
        | False -> Cons x xs
        | True -> dropWhile ds2 xs
        end
    end
  goal prop_35 :
    forall xs2 : list 'a4 . dropWhile constFalse xs2 = xs2
end
